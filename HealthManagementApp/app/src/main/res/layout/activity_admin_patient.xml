<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.adminUI.AdminPatient">

    <Button
        android:id="@+id/AdminPatient_btnSave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginBottom="10dp"
        android:text="@string/AdminPatient_btnSave"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/AdminPatient_btnDelete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="60dp"
        android:layout_marginRight="60dp"
        android:text="@string/AdminPatient_btnDelete"
        app:layout_constraintBottom_toBottomOf="@+id/AdminPatient_btnSave"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/AdminPatient_btnSave" />

    <EditText
        android:id="@+id/AdminPatient_etDiseases"
        android:layout_width="389dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:autofillHints=""
        android:background="@drawable/box"
        android:ems="10"
        android:gravity="center"
        android:inputType="textMultiLine"
        android:textColor="#18957C"
        app:layout_constraintStart_toStartOf="@+id/AdminPatient_tvDiseases"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_tvDiseases" />

    <TextView
        android:id="@+id/AdminPatient_tvFullName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/AdminPatient_tvFullName"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/AdminPatient_etPassword"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_etPassword" />

    <TextView
        android:id="@+id/AdminPatient_tvUserID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="@string/AdminPatient_tvUserID"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_tvPatientDetails" />

    <EditText
        android:id="@+id/AdminPatient_etPassword"
        android:layout_width="389dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:autofillHints=""
        android:background="@drawable/box"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPassword"
        android:textColor="#18957C"
        app:layout_constraintStart_toStartOf="@+id/AdminPatient_tvPassword"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_tvPassword" />

    <EditText
        android:id="@+id/AdminPatient_etPostalCode"
        android:layout_width="389dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:autofillHints=""
        android:background="@drawable/box"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:textColor="#18957C"
        app:layout_constraintStart_toStartOf="@+id/AdminPatient_tvPostalCode"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_tvPostalCode" />

    <TextView
        android:id="@+id/AdminPatient_tvAllergies"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/AdminPatient_tvAllergies"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/AdminPatient_etPostalCode"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_etPostalCode" />

    <TextView
        android:id="@+id/AdminPatient_tvPostalCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/AdminPatient_tvPostalCode"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/AdminPatient_etFullName"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_etFullName" />

    <TextView
        android:id="@+id/AdminPatient_tvPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/AdminPatient_tvPassword"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/AdminPatient_etUserID"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_etUserID" />

    <TextView
        android:id="@+id/AdminPatient_tvDiseases"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/AdminPatient_tvDiseases"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/AdminPatient_etAllergies"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_etAllergies" />

    <TextView
        android:id="@+id/AdminPatient_tvPatientDetails"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/AdminPatient_tvPatientDetails"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/AdminPatient_etFullName"
        android:layout_width="389dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:autofillHints=""
        android:background="@drawable/box"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:textColor="#18957C"
        app:layout_constraintStart_toStartOf="@+id/AdminPatient_tvFullName"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_tvFullName" />

    <EditText
        android:id="@+id/AdminPatient_etAllergies"
        android:layout_width="389dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:autofillHints=""
        android:background="@drawable/box"
        android:ems="10"
        android:gravity="center"
        android:inputType="textMultiLine"
        android:textColor="#18957C"
        app:layout_constraintStart_toStartOf="@+id/AdminPatient_tvAllergies"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_tvAllergies" />

    <EditText
        android:id="@+id/AdminPatient_etUserID"
        android:layout_width="389dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:autofillHints=""
        android:background="@drawable/box"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:textColor="#18957C"
        app:layout_constraintStart_toStartOf="@+id/AdminPatient_tvUserID"
        app:layout_constraintTop_toBottomOf="@+id/AdminPatient_tvUserID" />
</androidx.constraintlayout.widget.ConstraintLayout>